<h1>Select Players</h1>

<form class="trade-item-form">
  <tr>
    <th colspan="13">
      <h3>Requested Players</h3>
    </th>
  </tr>
  <table class="table trade-item-table">
    <tr>
      <th colspan="4">Name</th>
      <th>Opp</th>
      <th colspan="2">Rank</th>
      <th>2014 Stats</th>
      <th colspan="2">Fantasy</th>
      <th colspan="3">Availability</th>
    </tr>
    <tr>
      <th>[star]</th>
      <th>Name</th>
      <th>Pos</th>
      <th>Team</th>
      <th>Week ?</th>
      <th>Draft</th>
      <th>Pos</th>
      <th>Summary</th>
      <th>Total</th>
      <th>Avg</th>
      <th>%Own</th>
      <th>Status</th>
    </tr>
  </table>
  <input type="submit" class="btn btn-success" value="OK">
</form>
